  "messages":
    [ { "name": "Prefix",
        "locations":
          [ { "filename": "site.es6" } ],
        "docs":
          { "summary": "An object that contains information about the site's prefixes.",
            "description":
              [ "A site's routers, dynamic pages, and app pages all have prefixes." ],
            "links":
              [ "[SiteStructure](#SiteStructure)",
                "[currentPage](#currentPage)",
                "[getSiteStructure( )](#getSiteStructure)" ],
            "examples":
              [ { "title": "Get information about the site's prefixes",
                  "body":
                    [ "import wixSite from 'wix-site';",
                      "",
                      "// ...",
                      "",
                      "let structure = wixSite.getSiteStructure();",
                      "let prefixes = structure.prefixes;",
                      "",
                      "/*",
                      " *  prefixes:",
                      " *  [",
                      " *    {",
                      " *      \"name\": \"router\",",
                      " *      \"type\": \"router\",",
                      " *      \"prefix\": \"/router\"",
                      " *    },",
                      " *    {",
                      " *      \"name\": \"myCollection\",",
                      " *      \"type\": \"dynamicPages\",",
                      " *      \"prefix\": \"/myCollection\"",
                      " *    },",
                      " *    {",
                      " *      \"name\": \"myCollection\",",
                      " *      \"type\": \"dynamicPages\",",
                      " *      \"prefix\": \"/myCollection\"",
                      " *    },",
                      " *    {",
                      " *      \"name\": \"Shop\",",
                      " *      \"type\": \"app\",",
                      " *      \"prefix\": \"/shop\",",
                      " *      \"applicationId\": \"1380b703-ce81-ff05-f115-39571d94dfcd\"",
                      " *    },",
                      " *    {",
                      " *      \"name\": \"Product Page\",",
                      " *      \"type\": \"app\",",
